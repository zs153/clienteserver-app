    <div class="page-wrapper">
      <div class="page-body">
        <div class="container-xl">
          <div class="row row-deck row-cards">
            <div class="col-12">
              <div class="card">
                <% if(typeof alerts !='undefined' ) { %> <% alerts.map(function (alert)
                { %>
                <div
                  class="alert alert-warning alert-dismissible fade show"
                  role="alert"
                >
                  <%- alert.msg %>
                </div>
                <% }) %> <% } %>                
                <form action="/admin/oficinas/insert" method="POST">
                  <div class="card-header">
                    Oficina
                  </div>
                  <div class="card-body">
                    <div class="row">
                      <div class="col-8">
                        <div class="mb-3">
                          <label class="form-label">Descripción</label>
                          <input type="text" name="desofi" id="desofi" class="form-control text-uppercase" value="<%- datos.oficina.descripcion %>" />
                        </div>
                      </div>
                      <div class="col-4">
                        <div class="mb-3">
                          <label class="form-label">Código</label>
                          <input type="text" name="codofi" id="codofi" class="form-control" value="<%- datos.oficina.codigo %>" />
                        </div>
                      </div>
                    </div>
                  <div class="card-footer text-end">
                    <div class="d-flex">
                      <a href="/admin/oficinas" class="btn btn-link">Cancelar</a>
                      <button type="submit" class="btn btn-primary ms-auto" id="btnInsert">Crear nueva oficina</button>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    
    <footer class="footer footer-transparent d-print-none">
